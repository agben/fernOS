	%define ASCII_NL	0				;null
	%define ASCII_BS	8				;back space
	%define ASCII_LF	10				;line feed
	%define ASCII_CR	13				;carriage return
	%define ASCII_SP	32				;space
	%define ASCII_DQ	34				;double quotes
